let s:l = 16191 - ((5 * winheight(0) + 28) / 56)
if s:l < 1 | let s:l = 1 | endif
exe s:l
normal! zt
16191
let s:c = 80 - ((21 * winwidth(0) + 24) / 48)
if s:c > 0
  exe 'normal! 0' . s:c . 'lzs' . (80 - s:c) . 'l'
else
  normal! 080l
endif
let &so = s:so_save | let &siso = s:siso_save
